<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <lifecycle>
        <phase-listener>
            cn.hanbell.war.web.SignCheck
        </phase-listener>
    </lifecycle>
    <name>primefaces_ultima</name>
    <component>
        <component-type>org.primefaces.component.UltimaMenu</component-type>
        <component-class>org.primefaces.ultima.component.menu.UltimaMenu</component-class>
    </component>
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.UltimaMenuRenderer</renderer-type>
            <renderer-class>org.primefaces.ultima.component.menu.UltimaMenuRenderer</renderer-class>
        </renderer>
    </render-kit>
    <application>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>
    </application>
    <navigation-rule>
        <description>入口导航</description>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>resetPwd</from-outcome>
            <to-view-id>/resetPwd.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>404</from-outcome>
            <to-view-id>/404.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>deny</from-outcome>
            <to-view-id>/deny.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>系统整合平台一级导航</description>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <description>发票回执打印</description>
            <from-outcome>billimport</from-outcome>
            <to-view-id>/app/erp/billimport.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>billprint</from-outcome>
            <to-view-id>/app/erp/billprint.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>invmasimport</from-outcome>
            <to-view-id>/app/erp/invmasimport.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>ERP权限查询</description>
            <from-outcome>secusergrant</from-outcome>
            <to-view-id>/app/erp/secusergrant.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>发票签收回执打印</description>
        <from-view-id>/app/erp/billprint.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>billprintNew</from-outcome>
            <to-view-id>/app/erp/billprintNew.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>billprintEdit</from-outcome>
            <to-view-id>/app/erp/billprintEdit.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>billprintView</from-outcome>
            <to-view-id>/app/erp/billprintView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>ERP权限查询</description>
        <from-view-id>/app/erp/secusergrant.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>secusergrantView</from-outcome>
            <to-view-id>/app/erp/secusergrantView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>模态开窗</description>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>departmentSelect</from-outcome>
            <to-view-id>/app/df/departmentSelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>sysuserSelect</from-outcome>
            <to-view-id>/app/df/sysuserSelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cdrcusSelect</from-outcome>
            <to-view-id>/app/df/cdrcusSelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>